v-on=\"on\" dark relative fixed bottom right fab>\n              <v-icon>mdi-plus</v-icon>\n            </v-btn>\n          </v-fab-transition>\n        </v-card-text>\n      </template>\n\n      <v-card>\n        <v-card-title>\n          <span class=\"headline\">Adicionar Aula</span>\n        </v-card-title>\n        <v-card-text>\n          <v-container>\n            <v-row>\n              <v-col cols=\"12\">\n                <v-col cols=\"7\">\n                  <v-select\n                    :items = \"students\"\n                    item-value = \"id\"\n                    label = \"Estudante\"\n                    attach\n                    single-line\n                    v-model = \"schedulestudentprofessors.student\"\n                  >\n                  <template v-slot:selection=\"data\">\n                    {{ data.item.first_name }} {{ data.item.last_name }}\n                  </template>\n                  <template v-slot:item=\"data\">\n                    {{ data.item.first_name }} {{ data.item.last_name }}\n                  </template>\n                  </v-select>\n                </v-col>   \n              <v-col cols=\"7\">\n                <v-select\n                  :items = \"professors\"\n                  item-value = \"id\"\n                  item-text = \"first_name\"\n                  label = \"Professor\"\n                  attach\n                  single-line\n                  v-model = \"schedulestudentprofessors.professor\"\n                >\n                  <template v-slot:selection=\"data\">\n                    {{ data.item.first_name }} {{ data.item.last_name }}\n                  </template>\n                  <template v-slot:item=\"data\">\n                    {{ data.item.first_name }} {{ data.item.last_name }}\n                  </template>\n                </v-select>\n              </v-col>     \n                <v-col cols=\"7\">\n                  <v-select\n                    :items = \"roles\"\n        